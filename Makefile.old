CC:= g++
OBJS:= bubble_sort merge_sort quick_sort insertion_sort
CXXFLAGS:= -Wall -g -fopenmp \
	-Wall -Wextra \
	-D__GXX_EXPERIMENTAL_CXX0X__ \
	-I/usr/local/include/Cellar/glfw3/3.3/include/ \
	-I/usr/local/include/TSGL \
	-I/usr/local/include \
	-I/usr/local/include/freetype2 \
	-I/usr/local/include/freetype2/freetype \
	-std=c++11\
	-Wc++11-compat\
	-I/opt/AMDAPP/include/ \
	-I/usr/include/c++/4.6/ \
	-I/usr/include/c++/4.6/x86_64-linux-gnu/ \
	-I/usr/lib/gcc/x86_64-linux-gnu/9/include/ \
	-I/usr/include \
	-I/usr/include/freetype2 \
	-I/usr/include/freetype2/freetype
LDLIBS:= -ltsal

all: $(OBJS)

.PHONY: clean
clean:
	rm -rf $(OBJS)
